{
  "name": "bytic/payments",
  "type": "library",
  "license": "MIT",
  "description": "Payments module for bytic library",
  "keywords": [],
  "homepage": "https://github.com/ByTIC/payments",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/ByTIC/Common.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByTIC\\Payments\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\Payments\\Tests\\": "tests/src",
      "ByTIC\\Payments\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.0",
    "ext-zlib": "*",
    "bytic/common": "^0.9",
    "bytic/medialibrary": "^0.8",
    "bytic/omnipay-euplatesc": "^1.0",
    "bytic/omnipay-librapay": "^1.0",
    "bytic/omnipay-mobilpay": "^1.0",
    "bytic/omnipay-payu": "^1.0",
    "bytic/omnipay-romcard": "^1.0",
    "bytic/omnipay-twispay": "^1.0"
  },
  "require-dev": {
    "vlucas/phpdotenv": "^2.4",
    "bytic/facebook-pixel": "^1.0",
    "symfony/dom-crawler": "^3.3",
    "phpunit/phpunit": "^7.5",
    "mockery/mockery": "^1.2"
  },
  "suggest": {
    "bytic/facebook-pixel": "^1.0"
  }
}
